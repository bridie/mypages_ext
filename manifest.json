{
	"manifest_version": 2,
	"name": "My Pages",
	"version": "1",
	"description": "Save a page to mypages",
	"author": "Bridie Dibble",
	"browser_action": {
		"default_icon": "icon.png"
	},
	"background": {
		"scripts": ["jquery.js", "index.js"]
	},
	"permissions": [
		"activeTab",
		"http://localhost:3000/",
		"http://localhost:3000/*"
	]
}